<%= form_for(@article, html: {multipart: true}) do |f| %>
  <div>
    <p><%= f.text_field :title %></p>
    <p><%= f.text_area :body %></p>
    <p><%= f.text_field :tag_list %></p>
    <p>
    <%= f.label :image, "Attach an Image" %><br />
    <%= f.file_field :image %>
    </p>
    <%= f.submit %>
  </div>
<% end %>